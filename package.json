    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.2",
    "protractor": "^4.0.9",
    "rimraf": "^2.5.4",
    "systemjs-builder": "^0.15.33",
    "tslint": "^3.15.1",
